<div class="btn btn-primary btn-lg btn-block">
    <a asp-action="MetricImages" asp-route-id="@Model.Id" class="btn btn-primary btn-lg btn-block">Metrics<i class="glyphicon glyphicon-align-justify"></i></a>
</div>

<hr />
@*Xor operation between scramble image and generate image from key  *@
<h1> Xor operation between scramble image and generate image from key  </h1>
<table class="table">
    <thead class="thead-dark">
        <tr>
            <th scope="col">Scrambled Image</th>
            <th scope="col">Key Image </th>           
        </tr>
    </thead>
    <tbody>
        <tr>
            <th><img src="@Model.Scramble" width="250" height="200" /></th>
            <td><img src="@Model.Xorimage" width="250" height="200" /></td>           
        </tr>
    </tbody>
</table>

<div class="btn btn-primary btn-lg btn-block">
    <a asp-action="scrambledandxoroperationImagesmetrics" asp-route-id="@Model.Id" class="btn btn-primary btn-lg btn-block">Metrics<i class="glyphicon glyphicon-align-justify"></i></a>
</div>
<hr />
@*Chaotic5d *@
<h1> chaotic5d </h1>
<table class="table">
    <thead class="thead-dark">
        <tr>
            <th scope="col">Chaotic5d</th>
            
        </tr>
    </thead>
    <tbody>
        <tr>
            <th><img src="@Model.Chaotic5d" width="250" height="200" /></th>
           
        </tr>
    </tbody>
</table>
<div class="btn btn-primary btn-lg btn-block">
    <a asp-action="CahoticandscrambleImagesmetrics" asp-route-id="@Model.Id" class="btn btn-primary btn-lg btn-block">Metrics<i class="glyphicon glyphicon-align-justify"></i></a>
</div>
<hr />
@*generate image from scramble image and key image *@
<h1> generate image from scramble image and key image </h1>
<table class="table">
    <thead class="thead-dark">
        <tr>
            <th scope="col">New Image</th>
            <th scope="col">Swapping Bitmap ARGB Colour Channels</th>

        </tr>
    </thead>
    <tbody>
        <tr>
            <th><img src="@Model.GenerateImagefromscarmableandkeyimage" width="250" height="200" /></th>
            <th><img src="@Model.SwappingARGB" width="250" height="200" /></th>

        </tr>
    </tbody>
</table>
<hr />