@model HealthCare.API.Models.DetailViewModel

<div class="form-group">
    <label asp-for="diagonisicId" class="control-label"></label>
    <select asp-for="diagonisicId" asp-items="Model.diagonisics" class="form-control"></select>
    <span asp-validation-for="diagonisicId" class="text-danger"></span>
</div>

<div class="form-group">
    <label asp-for="Description" class="control-label"></label>
    <textarea asp-for="Description" class="form-control"></textarea>
    <span asp-validation-for="Description" class="text-danger"></span>
</div>



